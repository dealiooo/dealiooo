{
  "name": "client",
  "version": "0.1.0",
  "dependencies": {
    "bulma": "^0.7.2",
    "node-sass-chokidar": "^1.3.4",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.1",
    "socket.io-client": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --importer=node_modules/node-sass-tilde-importer --include-path ./src/scss --include-path ./node_modules src/scss/ --output src/",
    "watch-css": "npm run build-css && node-sass-chokidar --importer=node_modules/node-sass-tilde-importer --include-path ./src/scss --include-path ./node_modules src/scss/ --output src/ --watch --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "node-sass-tilde-importer": "^1.0.2"
  }
}
