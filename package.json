    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "connect-pg-simple": "^5.0.0",
    "cookie-parser": "^1.4.3",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "morgan": "^1.9.1",
